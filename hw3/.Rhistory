"Hobart",    -42.8821, 147.3272,
"Canberra",  -35.2809, 149.1300,
"Darwin",    -12.4634, 130.8456,
)
ggplot() +geom_sf(data=au_states) + geom_point(data=au_capitals, mapping = aes(x= lon, y = lat),color = 'red', size = 2)
