library(ggplot2) # keep in mind you have to install ggplot2 first via install.packages() if it's not installed in your machine
library(MASS)
data(Cars93)
head(Cars93)
ggplot(Cars93,aes(x = EngineSize,y= MPG.city)) + geom_point(col= "blue", size = 2) + labs(x = "Engine size", y = "Miles/gallon", title = "Engine size and miles/gallon in the city", subtitle = "This is the relationship between engine size and miles/gallon") + xlim(0.5,6) + ylim(10,50)
ggplot(Cars93,aes(x = EngineSize,y= MPG.city,col= Type)) + geom_point(size = 2) + labs(x = "Engine size", y = "Miles/gallon", title = "Engine size and miles/gallon in the city", subtitle = "This is the relationship between engine size and miles/gallon") + xlim(0.5,6) + ylim(10,50)
ggplot(Cars93,aes(x = EngineSize,y= MPG.city,col= Type)) + geom_point(size = 2) + labs(x = "Engine size", y = "Miles/gallon", title = "Engine size and miles/gallon in the city", subtitle = "This is the relationship between engine size and miles/gallon") + xlim(0.5,6) + ylim(10,50) + geom_smooth(linetype = 'dashed', col = 'red')
ggplot(Cars93, aes(AirBags, fill = AirBags)) + geom_bar()+ labs(title = "Cars' Airbag Equipment", subtitle = "This is the condition of cars and their airbags")
ggplot(Cars93, aes(x = Origin, fill = AirBags)) + geom_bar(position='dodge')+ labs(title = "Cars Origin and Cars' Airbag Equipment", subtitle = "This is the origin for the cars and their condition with airbags")
